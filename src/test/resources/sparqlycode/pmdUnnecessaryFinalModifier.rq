# Finds methods explicitly declared final inside final classes.
# Adam Nogradi, Interition Ltd.

PREFIX java: <http://www.interition.net/java/ref/>
PREFIX subject: <http://www.interition.net/sparqlycode/id/net/interition/sparqlycode/pmdtestsuite/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

ASK WHERE {
	SELECT (COUNT(?methodName) AS ?number)
		WHERE {
			subject:UnnecessaryFinalModifier java:isFinal true .
			subject:UnnecessaryFinalModifier java:method ?method .
			?method java:isFinal true .
			?method rdfs:label ?methodName .
		} HAVING (?number = 0)
}