# This finds all classes that are unusable because they have a 
# private constructor and no static methods.
# Adam Nogradi, Interition Ltd.

PREFIX java: <http://www.interition.net/java/ref/>
PREFIX subject: <http://www.interition.net/sparqlycode/id/net/interition/sparqlycode/pmdtestsuite/>

ASK  WHERE
{  
    subject:MissingStaticMethodInNonInstantiatableClass java:constructor ?constructor .
    subject:MissingStaticMethodInNonInstantiatableClass java:method ?method .
    ?constructor java:access "Private" .
    ?method java:isStatic true .
}