# This finds abstract classes that have no abstract methods in them.
# Adam Nogradi, Interition Ltd.

PREFIX java: <http://www.interition.net/java/ref/>
PREFIX subject: <http://www.interition.net/sparqlycode/id/net/interition/sparqlycode/pmdtestsuite/>


ASK WHERE {
		subject:AbstractClassWithoutAbstractMethod java:isAbstract true .
			FILTER EXISTS {
				subject:AbstractClassWithoutAbstractMethod java:method ?method .
				?method java:isAbstract true .
			} 
}
