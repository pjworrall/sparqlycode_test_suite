# This checks that constructors have same properties, where appropriate, as methods
# Paul Worrall, Interition Ltd.

PREFIX java: <http://www.interition.net/java/ref/>
PREFIX subject: <http://www.interition.net/sparqlycode/id/net/interition/sparqlycode/testsuite/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

ASK WHERE { 
	
   		?class rdfs:label "GeneralClassForConceptDetection" ;
   				a java:Class ;
   				java:constructor ?constructor .
   		# should test a few thing here really but access is enough for today
   		?constructor java:access ?access .
   		
}