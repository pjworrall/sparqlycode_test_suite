# Finds classes that extend Java.lang.Error
# Adam Nogradi, Interition Ltd.

PREFIX java: <http://www.interition.net/java/ref/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX subject: <http://www.interition.net/sparqlycode/id/net/interition/sparqlycode/pmdtestsuite/>

ASK WHERE {
			subject:DoNotExtendJavaLangError rdfs:label ?className .
			subject:DoNotExtendJavaLangError java:extends <http://www.interition.net/sparqlycode/id/java/lang/Error>
}
