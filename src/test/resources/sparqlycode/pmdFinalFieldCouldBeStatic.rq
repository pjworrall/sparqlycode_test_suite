# This finds classes with final field declarations that aren't static
# Returns True if offending fields are found
# Adam Nogradi, Interition Ltd.
# Modified: Paul Worrall

PREFIX java: <http://ontology.interition.net/java/ref/>
PREFIX subject: <http://www.interition.net/sparqlycode/id/net/interition/sparqlycode/pmdtestsuite/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

ASK WHERE {

	subject:FinalFieldCouldBeStatic java:field ?field .
	?field java:isFinal true .
	FILTER NOT EXISTS {?field java:isStatic true}

}