# This tests that arguments to a method that are arrays have dimension properties
# Paul Worrall, Interition Ltd.

# expecting:
#  getNames() has an argument of String[]

PREFIX java: <http://www.interition.net/java/ref/>
PREFIX net.interition.sparqlycode.testsuite: <http://www.interition.net/sparqlycode/id/net/interition/sparqlycode/testsuite/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>

ASK WHERE { 
	
   		net.interition.sparqlycode.testsuite:MethodArgumentExercise java:method ?method .
   		?method rdfs:label "getNames" ;
   		       java:argument ?argument .
   		?argument rdfs:label "surnames" .
   		
   		?argument java:dimension ?dimension .
   		
   		filter ( ?dimension = 1)
   		
}


