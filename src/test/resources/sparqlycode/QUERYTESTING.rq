# This finds methods and their respective classes with an excessive number of arguments. Eg 5

PREFIX java: <http://www.interition.net/java/ref/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX xsd: <http://www.example.org/>

SELECT ?class ?method (count(?argument) as ?totalArguments) WHERE
 {  
    ?method a java:Method .
    ?method java:argument ?argument .
    ?class java:method ?method .
}
group by ?class ?method
HAVING (?totalArguments > 4)
ORDER BY DESC (?totalArguments)
limit 100
